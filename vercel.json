{
  "version": 2,
  "buildCommand": "echo 'No build required for backend'",
  "outputDirectory": ".",
  "functions": {
    "api/test.js": {
      "maxDuration": 30
    },
    "api/organizations.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/test",
      "dest": "/api/test.js"
    },
    {
      "src": "/api/organizations",
      "dest": "/api/organizations.js"
    },
    {
      "src": "/api/organizations/(.*)",
      "dest": "/api/organizations.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "regions": ["iad1"]
}


{
  "version": 2,
  "buildCommand": "echo 'No build required for backend'",
  "outputDirectory": ".",
  "functions": {
    "api/index.mjs": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.mjs"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.mjs"
    }
  ]
}
